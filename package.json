{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "yarn lint:js && yarn lint:scss",
    "lint:js": "./node_modules/.bin/eslint --ext .js ./",
    "lint:scss": "./node_modules/.bin/stylelint --fix './src/**/*.scss'",
    "test": "jest --config ./jest.config.js",
    "storybook": "start-storybook -p 9001 -c ./src/packages/.storybook"
  },
  "dependencies": {
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme-to-json": "^3.3.3",
    "gatsby": "^1.9.241",
    "gatsby-link": "^1.6.39",
    "gatsby-paginate": "^1.0.14",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-sass": "^1.0.24",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.37",
    "node-polyglot": "^2.2.2",
    "query-string": "^6.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "slugify": "^1.2.9"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0"
  }
}
